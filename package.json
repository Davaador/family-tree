{
  "name": "family-tree",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^5.4.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.105",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "antd": "^5.21.5",
    "axios": "^1.7.4",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.12",
    "i18next": "^23.13.0",
    "rc-upload": "^4.8.1",
    "react": "^18.3.1",
    "react-app-rewired": "^2.2.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.8.2",
    "react-scripts": "5.0.1",
    "react-world-flags": "^1.6.0",
    "recharts": "^2.13.3",
    "web-vitals": "^2.1.4",
    "zustand": "^4.5.4"
  },
  "scripts": {
    "start": "HOST=0.0.0.0 react-app-rewired start",
    "start:dev": "cp env.development .env && HOST=0.0.0.0 react-app-rewired start",
    "start:prod": "cp env.production .env && HOST=0.0.0.0 react-app-rewired start",
    "build": "cp env.production .env && react-app-rewired build",
    "build:dev": "cp env.development .env && react-app-rewired build",
    "build:prod": "cp env.production .env && react-app-rewired build",
    "test": "react-app-rewired test",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "quality": "npm run type-check && npm run lint && npm run format:check",
    "test:coverage": "npm test -- --coverage --watchAll=false",
    "test:ci": "npm test -- --coverage --watchAll=false --passWithNoTests",
    "test:watch": "npm test -- --watch",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/react-world-flags": "^1.4.5",
    "autoprefixer": "^10.4.20",
    "babel-preset-react-app": "^10.0.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "less": "^4.2.0",
    "less-loader": "^12.2.0",
    "lint-staged": "^16.1.6",
    "postcss": "^8.4.49",
    "prettier": "^3.6.2",
    "react-i18next": "^14.0.1",
    "tailwindcss": "^3.4.14",
    "typescript": "^4.9.5",
    "webpack-bundle-analyzer": "^4.10.2",
    "workbox-webpack-plugin": "^7.3.0"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
